{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/workbox-(.*)",
      "dest": "/workbox-$1"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "/manifest.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
